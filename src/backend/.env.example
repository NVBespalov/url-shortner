# Версии
NODE_VERSION=18-alpine
NODE_ENV=production

# Порты
BACKEND_PORT=3000

# Пути
WORK_DIR=/app

# База данных
DB_HOST=postgres
DB_PORT=5432
DB_NAME=mydatabase
DB_USER=postgres
DB_PASSWORD=postgres

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=1h

# Логирование
LOG_LEVEL=info

# Настройки приложения
API_PREFIX=api
API_VERSION=v1
RATE_LIMIT_TTL=60000
RATE_LIMIT_MAX=100
